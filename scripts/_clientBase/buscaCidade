<script type="text/javascript" charset="iso-8859-1" language="javascript" src="html/js/jquery/jquery.js"></script>

<script type="text/javascript">
	function desabilitar(){
		return false
	}
	document.oncontextmenu=desabilitar

	setTimeout("document.getElementById('div_msg').style.display='none'",5000);

	if(window.ordenacao===undefined){
		var ordenacao = 'asc';
	}
	
	if(window.coluna===undefined){
		var coluna = '0';
	}
	
	$(document).ready(function() {
	  	$(".display tr").mouseover(function(){
			$(this).addClass("over")
		});
		$(".display tr").mouseout(function(){
			$(this).removeClass("over")
		});
		$('.display').dataTable( {
			"bJQueryUI": true,
			"sPaginationType": "full_numbers",
			"aaSorting": [[coluna,ordenacao]],
			"oLanguage": {
				"sUrl": "html/js/jquery/pt_BR"
			}
		} );

		$("#relatorio tr").mouseover(function(){
			$(this).addClass("over")
		});
		$("#relatorio tr").mouseout(function(){
			$(this).removeClass("over")
		});

		$('#relatorio').dataTable( {
			"bJQueryUI": true,
			"bPaginate": false,
			"sPaginationType": "full_numbers",
			"sDom": '<"H">tC<"F"p>',
			"oLanguage": {
				"sUrl": "html/js/jquery/pt_BR"
			}
		});
		
		$(function() {
			$("#accordion").accordion({
				autoHeight: false
			});
		});
	});
</script>		</div>
